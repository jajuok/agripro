{
  "name": "@agrischeme/mobile",
  "version": "0.1.0",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:components": "jest src/components",
    "test:stores": "jest src/store",
    "test:screens": "jest app/__tests__",
    "test:e2e": "maestro test .maestro/",
    "test:e2e:auth": "maestro test .maestro/auth/",
    "test:e2e:farms": "maestro test .maestro/farms/",
    "test:e2e:profile": "maestro test .maestro/profile/",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "detox:build:android:release": "detox build --configuration android.emu.release",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "detox:test:android:release": "detox test --configuration android.emu.release",
    "detox:test:android:reuse": "detox test --configuration android.emu.debug --reuse",
    "detox:clean": "detox clean-framework-cache && detox build-framework-cache",
    "e2e:android": "yarn detox:build:android && yarn detox:test:android",
    "e2e:android:ci": "yarn detox:build:android:release && yarn detox:test:android:release --cleanup --headless",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "build:pwa": "expo export --platform web && npx workbox-cli generateSW workbox-config.js"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-navigation/native": "^6.1.9",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.5",
    "expo": "~50.0.0",
    "expo-asset": "~9.0.2",
    "expo-camera": "~14.0.0",
    "expo-constants": "~15.4.0",
    "expo-document-picker": "~11.10.1",
    "expo-file-system": "~16.0.0",
    "expo-font": "~11.10.3",
    "expo-image-picker": "~14.7.0",
    "expo-linking": "~6.2.0",
    "expo-local-authentication": "~13.8.0",
    "expo-location": "~16.5.0",
    "expo-router": "~3.4.0",
    "expo-secure-store": "~12.8.0",
    "expo-splash-screen": "~0.26.0",
    "expo-status-bar": "~1.11.0",
    "expo-updates": "~0.24.13",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-maps": "1.8.0",
    "react-native-reanimated": "~3.6.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "^13.16.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^20.14.8",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "~50.0.0",
    "jsonfile": "^6.2.0",
    "msw": "^2.1.0",
    "pngjs": "^7.0.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.0",
    "workbox-cli": "^7.0.0"
  }
}
